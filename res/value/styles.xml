<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" tools:targetApi="17">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.    -->
   
    
     <style name="tv_et_left">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="BottomTabStyle">
        <item name="android:layout_width">190dp</item>
        <item name="android:layout_height">61.5dp</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="AppBaseTheme" parent="@android:style/Theme.Light.NoTitleBar.Fullscreen">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->

    </style>

    <style name="Seekbar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_light</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/scrubber_control_selector_holo</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:focusable">true</item>
        <item name="android:paddingStart">16dip</item>
        <item name="android:paddingEnd">16dip</item>
    </style>

    <style name="SoundSeekbar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/scrubber_progress_sound</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_sound</item>
        <item name="android:minHeight">9dp</item>
        <item name="android:maxHeight">9dp</item>
        <item name="android:thumb">@drawable/sound_control</item>
        <item name="android:thumbOffset">10dip</item>
        <item name="android:focusable">true</item>
        <item name="android:paddingStart">6dip</item>
        <item name="android:paddingEnd">10dip</item>
    </style>

    <style name="Actived" parent="@android:style/Theme.Light">
        <item name="android:background">?android:attr/activatedBackgroundIndicator</item>
    </style>

    <style name="NoActionBar" parent="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">
        <item name="android:buttonStyle">@style/Button.Green</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="MainTheme" parent="@android:style/Theme.Holo.NoActionBar.Fullscreen">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowShowWallpaper">true</item>
    </style>

    <style name="Button" parent="@android:style/Widget.Holo.Light.Button">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:paddingLeft">24dp</item>
        <item name="android:paddingRight">24dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:minHeight">36dp</item>
    </style>

    <style name="Button.Green">
        <item name="android:background">@drawable/btn_green</item>
    </style>

    <style name="Button.Red">
        <item name="android:background">@drawable/btn_red</item>
    </style>

    <style name="Achievement" parent="@android:style/TextAppearance.Holo.Large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:text">0</item>
        <item name="android:maxLines">1</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="PopupTranslation">

        <!--
        <item name="android:windowEnterAnimation">@anim/enter</item>
        <item name="android:windowExitAnimation">@anim/exit</item>
        -->
        <item name="android:windowEnterAnimation">@anim/show</item>
        <item name="android:windowExitAnimation">@anim/hide</item>
    </style>

    <style name="InputBox" parent="@android:style/Widget.Holo.Light.EditText">
        <item name="android:maxLength">8</item>
        <item name="android:minWidth">200dp</item>
        <item name="android:minHeight">36dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">4dp</item>
        <item name="android:background">@drawable/rectangle_gray</item>
    </style>

    <style name="Label">
        <item name="android:layout_width">@dimen/label_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">20dp</item>
    </style>

    <style name="Labelll">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="MissionListItem"></style>

    <style name="DayText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text">abc</item>
        <item name="android:padding">@dimen/daytext_padding</item>
    </style>

    <style name="Week">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

  
    <style name="PassWord" parent="@android:style/Widget.Holo.Light.Button">
        <item name="android:layout_width">58.5dp</item>
        <item name="android:layout_height">58.5dp</item>
        <item name="android:background">@drawable/bg_pw</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">0dp</item>
        <item name="android:textSize">@dimen/frag_textsize_normal</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:maxLines">1</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MyDialog" parent="@android:style/Theme.Holo.Light.Dialog.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowMinWidthMajor">48dp</item>
        <item name="android:windowMinWidthMinor">48dp</item>
    </style>

    <style name="resttingtheme" parent="@android:style/Theme.Holo.Light">

        <!-- 去掉状态栏 -->
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    

</resources>